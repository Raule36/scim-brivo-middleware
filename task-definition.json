{
  "family": "brivo-scim-gateway",
  "networkMode": "awsvpc",
  "requiresCompatibilities": ["FARGATE"],
  "cpu": "256",
  "memory": "2048",
  "executionRoleArn": "arn:aws:iam::136337412719:role/ecsTaskExecutionRole",
  "containerDefinitions": [
    {
      "name": "brivo-scim-gateway",
      "image": "nginx:alpine",
      "essential": true,
      "portMappings": [
        {
          "containerPort": 3000,
          "protocol": "tcp"
        }
      ],
      "environment": [
        { "name": "PORT", "value": "3000" },
        { "name": "NODE_ENV", "value": "production" }
      ],
      "secrets": [
        { "name": "DB_HOST", "valueFrom": "arn:aws:secretsmanager:eu-central-1:136337412719:secret:prod/brivo-scim-gateway/Porstgres-pZRPYB:host::" },
        { "name": "DB_PORT", "valueFrom": "arn:aws:secretsmanager:eu-central-1:136337412719:secret:prod/brivo-scim-gateway/Porstgres-pZRPYB:port::" },
        { "name": "DB_USER", "valueFrom": "arn:aws:secretsmanager:eu-central-1:136337412719:secret:prod/brivo-scim-gateway/Porstgres-pZRPYB:username::" },
        { "name": "DB_PASSWORD", "valueFrom": "arn:aws:secretsmanager:eu-central-1:136337412719:secret:prod/brivo-scim-gateway/Porstgres-pZRPYB:password::" },
        { "name": "DB_NAME", "valueFrom": "arn:aws:secretsmanager:eu-central-1:136337412719:secret:prod/brivo-scim-gateway/Porstgres-pZRPYB:dbInstanceIdentifier::" }
      ],
      "logConfiguration": {
        "logDriver": "awslogs",
        "options": {
          "awslogs-group": "/ecs/",
          "awslogs-create-group": "true",
          "awslogs-region": "eu-central-1",
          "awslogs-stream-prefix": "ecs"
        }
      }
    }
  ]
}